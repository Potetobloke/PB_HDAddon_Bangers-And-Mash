//-----------------
// The Great Menu
//-----------------

/* This is organized in the way of:
- Adding the option menu to options and the HDAddonMenu.
- Main menu, where everything is housed.
- Specific weapon menus.
*/

// Main Menu stuff
AddOptionMenu "OptionsMenu" {
	Submenu "$PBWEPS_MENU", "PBWepsMenu"
}

AddOptionMenu "HDAddonMenu" {
	Submenu "$PBWEPS_MENU", "PBWepsMenu"
}

OptionMenu "PBWepsMenu" {
	Title "$PBWEPS_MENU"
    StaticText ""

    StaticText "Hey, this shit's work in progress! Get outta here! - [Ted]"
	StaticText ""
	
	StaticText "$MENU_PISTOLS_TITLE", "Yellow"
	StaticText ""
    Submenu "$TAG_AUTOMAG", "automagSpawning", 0, 1
	StaticText ""
    Submenu "$TAG_MK23", "mk23Spawning", 0, 1
	StaticText ""

	StaticText "$MENU_SHOTGUNS_TITLE", "Red"
    StaticText ""
    Submenu "$TAG_CAWS", "cawsSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_STREETSWEEPER", "streetsweeperSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_URAGAN5", "uragan5Spawning", 0, 1
	StaticText ""

	StaticText "$MENU_MACHINEGUNS_TITLE", "Green"
	StaticText ""
    Submenu "$TAG_SMARTGUN", "smartgunSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_KELENKEN", "kelenkenSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_M60", "m60Spawning", 0, 1
	StaticText ""
	Submenu "$TAG_FAL", "falSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_M5165C", "m5165cSpawning", 0,1
	StaticText ""
	Submenu "$TAG_MAC10", "mac10Spawning", 0,1
	StaticText ""

	StaticText "$MENU_EXPLOSIVES_TITLE", "Brown"
	StaticText ""
    Submenu "$TAG_EXAMPLE", "example", 0, 1
	StaticText ""

	StaticText "$MENU_RIFLES_TITLE", "Orange"
	StaticText ""
    Submenu "$TAG_DEVINCENZIA", "devincenziaSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_STEYRACR", "steyracrSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_G11", "g11Spawning", 0, 1
	StaticText ""
	Submenu "$TAG_LEVERGAT", "levergatSpawning", 0, 1
	StaticText ""

    StaticText "$MENU_ENERGYWEAPONS_TITLE", "Cyan"
	StaticText ""
    Submenu "$TAG_RAILGUN", "railgunSpawning", 0, 1
	StaticText ""

    StaticText "$MENU_INCENDIARYWEAPONS_TITLE", "Fire"
	StaticText ""
    Submenu "$TAG_MANCUBUSCANNON", "mancubuscannonSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_WPGRENADE", "wpgrenadeSpawning", 0, 1
	StaticText ""

	StaticText "$MENU_MELEEWEAPONS_TITLE", "DarkGrey"
	StaticText ""
	Submenu "$TAG_KNIFE", "knifeSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_FIREAXE", "fireaxeSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_SLEDGEHAMMER", "sledgehammerSpawning", 0, 1
	StaticText ""

	StaticText "$MENU_RESETOPTIONS_TITLE", "Red"
	StaticText ""
	SafeCommand "$PBWEPS_RESETALLOPTIONS", "PbWeps_Reset_Cvars"
}

// Spawn Bias (for items that do replace)
OptionValue "SpawnBias" {
	-1,    "Disabled"
	 0,    "Replace All"
	 2,    "1 in 3"
	 4,    "1 in 5"
	 9,    "1 in 10"
	 14,   "1 in 15"
	 19,   "1 in 20"
	 24,   "1 in 25"
	 49,   "1 in 50"
	 99,   "1 in 100"
	 149,  "1 in 150"
	 199,  "1 in 200"
}

// 4mm Firearms
OptionMenu "g11Spawning" {
    Title "$MENU_G11_TITLE"
    StaticText ""

   	StaticText "$MENU_NOTICE", "Orange"
    StaticText "$MENU_SPAWNRATE_TEXT1", "White"
    StaticText "$MENU_SPAWNRATE_TEXT2", "White"
    StaticText ""

	StaticText "$MENU_WEPSPAWNOPTIONS", "Orange"
    StaticText "$MENU_G11_NORMAL_VULCANETTE_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "g11_normal_vulcanette_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar g11_normal_vulcanette_spawn_bias"
 	StaticText ""

	StaticText "$MENU_WEPSPAWNOPTIONS", "Orange"
    StaticText "$MENU_G11_SCOPED_VULCANETTE_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "g11_scoped_vulcanette_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar g11_scoped_vulcanette_spawn_bias"
 	StaticText ""

	StaticText "$MENU_G11_BACKPACKS", "White"
	Option "$MENU_BACKPACKSALLOWED", "g11_allowBackpacks", "OnOff"
 	StaticText ""

	StaticText "$MENU_PERSISTENCYOPTIONS", "Orange"
    StaticText "$MENU_PERSISTENCY_TEXT", "White"
	Option "$MENU_WEPAUTOMAGFAPERSISTENCY", "g11_normal_persistent_spawning", "OnOff"
	Option "$MENU_WEPAUTOMAGSAPERSISTENCY", "g11_scoped_persistent_spawning", "OnOff"
	SafeCommand "$MENU_RESETPERSISTENCY", "resetcvar g11_normal_persistent_spawning; resetcvar g11_scoped_persistent_spawning"
 	StaticText ""

	StaticText "$MENU_RESETOPTIONS", "Orange"
	SafeCommand "$MENU_RESETALLOPTIONS", "g11_reset_cvars"
}

