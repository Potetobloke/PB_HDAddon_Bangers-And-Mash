//-----------------
// The Great Menu
//-----------------

/* This is organized in the way of:
- Adding the option menu to options and the HDAddonMenu.
- Main menu, where everything is housed.
- Specific weapon menus.*/

// Main Menu stuff
AddOptionMenu "OptionsMenu" {
	Submenu "$PBWEPS_MENU", "PBWepsMenu"
}

AddOptionMenu "HDAddonMenu" {
	Submenu "$PBWEPS_MENU", "PBWepsMenu"
}

// Spawn Bias (for items that do replace)
OptionValue "SpawnBias" {
	-1,    "Disabled"
	 0,    "Replace All"
	 2,    "1 in 3"
	 4,    "1 in 5"
	 9,    "1 in 10"
	 14,   "1 in 15"
	 19,   "1 in 20"
	 24,   "1 in 25"
	 49,   "1 in 50"
	 99,   "1 in 100"
	 149,  "1 in 150"
	 199,  "1 in 200"
}

OptionMenu "PBWepsMenu" {
	Title "$PBWEPS_MENU"
    StaticText ""
	
	StaticText "$MENU_PISTOLS_TITLE", "Yellow"
	StaticText ""
    Submenu "$TAG_AUTOMAG", "automagSpawning", 0, 1
	StaticText ""
    Submenu "$TAG_MK23", "mk23Spawning", 0, 1
	StaticText ""
	Submenu "$TAG_93R", "93rSpawning", 0, 1
	StaticText ""

	StaticText "$MENU_SHOTGUNS_TITLE", "Red"
    StaticText ""
    Submenu "$TAG_CAWS", "cawsSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_STREETSWEEPER", "streetsweeperSpawning", 0, 1
	StaticText ""
	Submenu "$TAG_URAGAN5", "uragan5Spawning", 0, 1
	StaticText ""
