	SafeCommand "$PBWEPS_RESETALLOPTIONS", "pbweps_reset_cvars"
}

// 4mm Firearms
OptionMenu "g11Spawning" {
    Title "$MENU_G11_TITLE"
    StaticText ""

   	StaticText "$MENU_NOTICE", "Orange"
    StaticText "$MENU_SPAWNRATE_TEXT1", "White"
    StaticText "$MENU_SPAWNRATE_TEXT2", "White"
    StaticText ""

	StaticText "$MENU_WEPSPAWNOPTIONS", "Orange"
    StaticText "$MENU_G11_NORMAL_VULCANETTE_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "g11_normal_vulcanette_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar g11_normal_vulcanette_spawn_bias"
 	StaticText ""

	StaticText "$MENU_WEPSPAWNOPTIONS", "Orange"
    StaticText "$MENU_G11_SCOPED_VULCANETTE_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "g11_scoped_vulcanette_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar g11_scoped_vulcanette_spawn_bias"
 	StaticText ""

	StaticText "$MENU_G11_BACKPACKS", "White"
	Option "$MENU_BACKPACKSALLOWED", "g11_allowBackpacks", "OnOff"
 	StaticText ""

	StaticText ""
	StaticText "$MENU_MAGSPAWNOPTIONS", "Orange"
    StaticText "$MENU_G11_MAGAZINE_SHELLBOX_SPAWNTEXT", "White"
	Option "$MENU_MAGSPAWNRATE", "g11mag_shellbox_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETMAG", "resetcvar g11mag_shellbox_spawn_bias"
	// Till I confirm that this actually works. It probably doesn't. - [Ted]
	/*StaticText "$MENU_G11_MAGAZINE_CLIPBOX_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "g11mag_clipbox_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar g11mag_clipbox_spawn_bias"*/
 	StaticText ""

	StaticText "$MENU_G11_BACKPACKS", "White"
	Option "$MENU_BACKPACKSALLOWED", "g11_allowBackpacks", "OnOff"
	StaticText "$MENU_G11_MAG_BACKPACKS", "White"
	Option "$MENU_BACKPACKSALLOWED", "g11mag_allowBackpacks", "OnOff"
 	StaticText ""

	StaticText "$MENU_PERSISTENCYOPTIONS", "Orange"
    StaticText "$MENU_PERSISTENCY_TEXT", "White"
	Option "$MENU_WEPG11NORMALPERSISTENCY", "g11_normal_persistent_spawning", "OnOff"
	Option "$MENU_WEPG11SCOPEDPERSISTENCY", "g11_scoped_persistent_spawning", "OnOff"
	Option "$MENU_MAGPERSISTENCY", "g11mag_persistent_spawning", "OnOff"
	SafeCommand "$MENU_RESETPERSISTENCY", "resetcvar g11_normal_persistent_spawning; resetcvar g11_scoped_persistent_spawning; resetcvar g11mag_persistent_spawning"
 	StaticText ""

	StaticText "$MENU_RESETOPTIONS", "Orange"
	SafeCommand "$MENU_RESETALLOPTIONS", "g11_reset_cvars"
}

